    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "es5"
  },
  "devDependencies": {
    "@types/node": "^12.0.10",
    "@types/react": "^16.8.22",
    "@types/react-dom": "^16.8.4",
    "now": "^15.6.2",
    "prettier": "^1.18.2"
